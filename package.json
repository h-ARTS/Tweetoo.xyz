{
  "name": "tweetoo.xyz",
  "version": "1.0.0",
  "description": "A Twitter Clone",
  "main": "index.js",
  "scripts": {
    "build": "babel api --out-dir dist",
    "dev": "nodemon --exec yarn restart",
    "test-router": "jest -t router:",
    "test-controllers": "jest -t controllers:",
    "test-model": "jest -t model:",
    "test-auth": "jest -t Authentication: --detectOpenHandles",
    "test-crud": "jest /api/utils/__tests__/crud.spec.js",
    "restart": "rimraf dist && yarn build && yarn start",
    "start": "node --inspect dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h-ARTS/Tweetoo.xyz.git"
  },
  "keywords": [
    "Twitter",
    "Clone",
    "Clone",
    "app",
    "test"
  ],
  "author": "Hanan Khan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/h-ARTS/Tweetoo.xyz/issues"
  },
  "homepage": "https://github.com/h-ARTS/Tweetoo.xyz#readme",
  "dependencies": {
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "busboy-body-parser": "^0.3.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongo-gridfs": "^1.1.0",
    "mongoose": "^5.8.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.4.2",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test-db-setup.js"
    ],
    "testPathIgnorePatterns": [
      "dist/"
    ],
    "restoreMocks": true
  }
}
