    "express": "^4.17.1",
    "mongoose": "^5.8.0",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
