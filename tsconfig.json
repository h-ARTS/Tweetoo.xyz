{
  "compilerOptions": {
    "outDir": "./dist",
    "moduleResolution": "node",
    "module": "commonjs",
    "allowJs": true,
    "target": "es6",
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
  },
  "include": ["./api/**/*", "./config/*"],
  "exclude": ["node_modules"]
}
